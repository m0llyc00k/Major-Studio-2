ode/Thesis_folders/Thesis-2022/vue-app-mc/vue-scrollama/src/Scrollama.vue"],"sourcesContent":["<template>\n  <div class=\"scrollama__steps\">\n    <slot />\n  </div>\n</template>\n\n<script>\nimport scrollama from \"scrollama\";\n\nexport default {\n  inheritAttrs: false,\n  name: \"Scrollama\",\n  mounted() {\n    this._scroller = scrollama();\n    this.setup();\n  },\n  beforeUnmount() {\n    this._scroller.destroy();\n  },\n  computed: {\n    opts() {\n      return Object.assign(\n        {},\n        {\n          step: Array.from(this.$el.children),\n          progress: !!this.$attrs[\"step-progress\"],\n        },\n        this.$attrs\n      );\n    },\n  },\n  methods: {\n    setup() {\n      this._scroller.destroy();\n\n      this._scroller\n        .setup(this.opts)\n        .onStepProgress((resp) => {\n          this.$emit(\"step-progress\", resp);\n        })\n        .onStepEnter((resp) => {\n          this.$emit(\"step-enter\", resp);\n        })\n        .onStepExit((resp) => {\n          this.$emit(\"step-exit\", resp);\n        });\n\n      window.addEventListener(\"resize\", this.handleResize);\n    },\n    handleResize() {\n      this._scroller.resize();\n    },\n  },\n};\n</script>\n"]}]}