     })\n        .onStepExit((resp) => {\n          this.$emit(\"step-exit\", resp);\n        });\n\n      window.addEventListener(\"resize\", this.handleResize);\n    },\n    handleResize() {\n      this._scroller.resize();\n    },\n  },\n};\n</script>\n"]}]}